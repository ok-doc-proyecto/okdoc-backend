{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Edge construida automaticamente",
      "request": "launch",
      "type": "msedge",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch Edge construida a mano",
      "request": "launch",
      "type": "msedge",
      "url": "http://localhost:7000/ws/async/",
      "webRoot": "${workspaceFolder/project/tryasync/templates/}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "*": "${webRoot}/*"
      }
    },
    {
      "name": "Iniciar app chrome ",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:7000/ws/async/",
      "webRoot": "${workspaceFolder/chats/templates/}",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "*": "${webRoot}/*"
      }
    },
    {
      "name": "Iniciar Servidor desarrollo",
      "type": "python",
      "request": "launch",
      "envFile": "${workspaceFolder/.venv}",
      "program": "${workspaceFolder}\\manage.py",
      "args": ["runserver", "7000"],
      "django": true,
      "justMyCode": false
    },
    {
      "name": "Iniciar Hypercorn",
      "type": "python",
      "request": "launch",
      "envFile": "${workspaceFolder/.venv}",
      "module": "hypercorn",
      "args": [
        "project/okdoc.asgi:application",
        "--reload",
        "--bind",
        "localhost:7000"
      ],
      "django": true,
      "justMyCode": false
    },
    {
      "type": "pwa-msedge",
      "name": "Launch Microsoft Edge",
      "request": "launch",
      "runtimeArgs": ["--remote-debugging-port=9222"],
      "url": "c:\\Users\\ricardogcba\\.vscode\\extensions\\ms-edgedevtools.vscode-edge-devtools-2.1.3\\out\\startpage\\index.html", // Provide your project's url to finish configuring
      "presentation": {
        "hidden": true
      }
    },
    {
      "type": "pwa-msedge",
      "name": "Launch Microsoft Edge in headless mode",
      "request": "launch",
      "runtimeArgs": ["--headless", "--remote-debugging-port=9222"],
      "url": "c:\\Users\\ricardogcba\\.vscode\\extensions\\ms-edgedevtools.vscode-edge-devtools-2.1.3\\out\\startpage\\index.html", // Provide your project's url to finish configuring
      "presentation": {
        "hidden": true
      }
    },
    {
      "type": "vscode-edge-devtools.debug",
      "name": "Open Edge DevTools",
      "request": "attach",
      "url": "c:\\Users\\ricardogcba\\.vscode\\extensions\\ms-edgedevtools.vscode-edge-devtools-2.1.3\\out\\startpage\\index.html", // Provide your project's url to finish configuring
      "presentation": {
        "hidden": true
      }
    }
  ],
  "compounds": [
    {
      "name": "Launch Edge Headless and attach DevTools",
      "configurations": [
        "Launch Microsoft Edge in headless mode",
        "Open Edge DevTools"
      ]
    },
    {
      "name": "Launch Edge and attach DevTools",
      "configurations": ["Launch Microsoft Edge", "Open Edge DevTools"]
    }
  ]
}
